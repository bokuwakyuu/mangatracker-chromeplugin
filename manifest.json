{
  "name": "Manga tracker",
  "version": "1.0",
  "background_page": "background.html",
  "options_page": "api-key.html",
  "icons": {  "16": "icon16.png",
              "48": "icon48.png",
              "128": "icon128.png" },
  
  "browser_action" : {
    "default_icon": "icon16.png"
  },
  
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_end",
      "js": ["jquery.js", "myscript.js"],
      "css": ["css.css"]
    }
  ],
  "permissions": ["tabs", "http://*/*", "*", "http://localhost:9000/*"]
}

